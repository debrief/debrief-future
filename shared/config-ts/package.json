{
  "name": "@debrief/config",
  "version": "0.1.0",
  "description": "Shared configuration service for Debrief v4.x (TypeScript)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
